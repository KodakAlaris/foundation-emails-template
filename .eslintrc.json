{
    "rules": {
        "eqeqeq": 1,
        "indent": [
            2,
            4,
            {
                "SwitchCase": 1
            }
        ],
        "no-console": 1,
        "quotes": [
            2,
            "double"
        ],
        "linebreak-style": [
            2,
            "windows"
        ],
        "semi": [
            2,
            "always"
        ],
        "strict": [
            2,
            "global"
        ]
    },
    "env": {
        "es6": true,
        "browser": true,
        "commonjs": true,
        "node": true,
        "worker": true
    },
    "extends": "eslint:recommended",
    "globals": {},
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 6
    }
}